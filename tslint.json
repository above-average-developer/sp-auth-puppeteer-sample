{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "always", "ignore-interfaces"],
    "no-string-literal": false,
    "strict-type-predicates": false,
    "no-console": false,
    "trailing-comma": false,
    "ordered-imports": false
  }
}
