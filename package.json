  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/puppeteer": "^2.0.0",
    "@types/node": "^13.7.0",
    "rimraf": "^3.0.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.5"
